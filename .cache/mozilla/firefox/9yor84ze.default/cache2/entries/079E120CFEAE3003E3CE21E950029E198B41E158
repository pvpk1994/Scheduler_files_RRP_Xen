                if (opt.firefox13) {
                    ss.animate({
                        'top': (0 + (opt.height)) + "px",
                        'opacity': 1
                    }, {
                        duration: masterspeed + ((opt.slots - i) * (70 - opt.slots)),
                        queue: false,
                        complete: function () {}
                    });
                } else {
                    ss.cssAnimate({
                        'top': (0 + (opt.height)) + "px",
                        'opacity': 1
                    }, {
                        duration: masterspeed + ((opt.slots - i) * (70 - opt.slots)),
                        queue: false,
                        complete: function () {}
                    });
                }
            });
            nextsh.find('.slotslide').each(function (i) {
                var ss = $(this);
                ss.css({
                    'top': (0 - (opt.height)) + "px",
                    'opacity': 0
                });
                if (opt.firefox13) {
                    ss.animate({
                        'top': '0px',
                        'opacity': 1

                        duration: masterspeed + ((opt.slots - i) * (70 - opt.slots)),
                        queue: false,
                        complete: function () {
                            if (i == 0) {
                                removeSlots(container, opt);
                                nextsh.find('.defaultimg').css({
                                    'opacity': 1
                                });
                                actsh.find('.defaultimg').css({
                                    'opacity': 0
                                });
                                opt.act = opt.next;
                                moveSelectedThumb(container);
                            }
                        }
                    });
                } else {
